{
    "jinja": {
        "extends": "html",
        "snippets": {
            "stm": "{% ${child}| %}",
            "expr": "{{ ${child}| }}",
            "cmt": "{# ${child}| #}",
            "blk": "{% block ${child} %}\n|\n{% endblock %}",
            "blki": "{% block ${child} %}|{% endblock %}",
            "blkie": "{% block ${child} %}{% endblock %}|",
            "extds": "{% extends \"${child}|\" %}"
        }
    }
}
